{
  "name": "vue_app",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "4.5.0",
    "bootstrap-vue": "2.15.0",
    "chart.js": "2.9.3",
    "core-js": "3.6.5",
    "flatpickr": "4.6.3",
    "nouislider": "14.6.0",
    "package.json": "^2.0.1",
    "register-service-worker": "1.7.1",
    "vue": "2.6.11",
    "vue-chartjs": "3.5.0",
    "vue-clipboard2": "0.3.1",
    "vue-flatpickr-component": "8.1.5",
    "vue-router": "3.3.4",
    "vue2-transitions": "0.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "lint-staged": "^6.0.0",
    "vue-template-compiler": "2.6.11",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
